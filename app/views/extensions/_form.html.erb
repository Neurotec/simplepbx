<%= simple_form_for(@extension) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :endpoint, collection: Endpoint.from_user(current_user)%>
    <%= f.input :username %>
    <%= f.input :password, as: :string %>
    <%= f.input :cid_name %>
    <%= f.input :cid_number %>
    <%= f.input :location %>
    <%= f.input :do_record %>
    <%= f.association :extension_profile %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
