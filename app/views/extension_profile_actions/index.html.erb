<p id="notice"><%= notice %></p>

<h1>Extension Profile Actions</h1>

<table>
  <thead>
    <tr>
      <th>Extension profile</th>
      <th>Order</th>
      <th>Application</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extension_profile_actions.each do |extension_profile_action| %>
      <tr>
        <td><%= extension_profile_action.extension_profile %></td>
        <td><%= extension_profile_action.order %></td>
        <td><%= extension_profile_action.application %></td>
        <td><%= extension_profile_action.data %></td>
        <td><%= link_to 'Show', extension_profile_action %></td>
        <td><%= link_to 'Edit', edit_extension_profile_action_path(extension_profile_action) %></td>
        <td><%= link_to 'Destroy', extension_profile_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extension Profile Action', new_extension_profile_action_path %>
