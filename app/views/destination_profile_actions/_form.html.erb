<%= simple_form_for(@destination_profile_action) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :destination_profile %>
    <%= f.input :application %>
    <%= f.input :data %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
